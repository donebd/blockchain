    "crypto-js": "^4.1.1",
    "properties-reader": "^2.2.0",
    "randomstring": "^1.2.3",
    "ts-node": "^10.9.1",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/properties-reader": "^2.1.1",
    "@types/randomstring": "^1.1.8",
    "@types/ws": "^8.5.4"
